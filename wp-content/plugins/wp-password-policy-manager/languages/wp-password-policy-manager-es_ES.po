msgid "Invalid Request"
msgstr "Petición inválida"

#: wp-password-policy-manager.php:215
msgid "<strong>ERROR</strong>: The new password cannot be empty."
msgstr "<strong>ERROR</strong>: La contraseña nueva no puede estar vacía."

#: wp-password-policy-manager.php:217
msgid "<strong>ERROR</strong>: Both new passwords must match."
msgstr "<strong>ERROR</strong>: Las dos contraseñas deben coincidir."

#: wp-password-policy-manager.php:219
msgid "<strong>ERROR</strong>: New password cannot be the same as the old one."
msgstr "<strong>ERROR</strong>: La contraseña nueva no puede ser igual que la anterior."

#: wp-password-policy-manager.php:221 wp-password-policy-manager.php:467
msgid "<strong>ERROR</strong>: New password cannot be the same as the username."
msgstr "<strong>ERROR</strong>: La contraseña nueva no puede ser igual que el nombre de usuario."

#: wp-password-policy-manager.php:223 wp-password-policy-manager.php:471
msgid "<strong>ERROR</strong>: New password cannot be the same as the email."
msgstr "<strong>ERROR</strong>: La contraseña nueva no puede ser igual que el email."

#: wp-password-policy-manager.php:226 wp-password-policy-manager.php:371
#: wp-password-policy-manager.php:476
msgid "<strong>ERROR</strong>: New password must contain at least %d characters."
msgstr "<strong>ERROR</strong>: La contraseña nueva tiene que contener al menos %d caracteres."

#: wp-password-policy-manager.php:229 wp-password-policy-manager.php:377
#: wp-password-policy-manager.php:482
msgid "<strong>ERROR</strong>: New password must contain both uppercase and lowercase characters."
msgstr "<strong>ERROR</strong>: La contraseña nueva tiene que contener mayúsculas y minúsculas."

#: wp-password-policy-manager.php:232 wp-password-policy-manager.php:383
#: wp-password-policy-manager.php:488
msgid "<strong>ERROR</strong>: New password must contain numbers."
msgstr "<strong>ERROR</strong>: La contraseña nueva debe contener números."

#: wp-password-policy-manager.php:235 wp-password-policy-manager.php:389
#: wp-password-policy-manager.php:494
msgid "<strong>ERROR</strong>: New password must contain special characters."
msgstr ""
"<strong>ERROR</strong>: La contraseña nueva debe contener caracteres especiales (por ejemplo: .,!"
"#$_+)."

#: wp-password-policy-manager.php:241 wp-password-policy-manager.php:403
#: wp-password-policy-manager.php:504
msgid "<strong>ERROR</strong>: New password must not be one of the previous %d used passwords."
msgstr ""
"<strong>ERROR</strong>: La contraseña nueva no puede ser igual a una de las %d contraseñas "
"previas."

#: wp-password-policy-manager.php:258
msgid "1 minute"
msgstr "1 minuto"

#: wp-password-policy-manager.php:261
msgid "<strong>ERROR</strong>: The password you entered expired %s ago."
msgstr "<strong>ERROR</strong>: La contraseña ha expirado hace %d."

#: wp-password-policy-manager.php:273
msgid "Current Password"
msgstr "Contraseña actual"

#: wp-password-policy-manager.php:276
msgid "Type your current password to be able to change your password."
msgstr "Teclea tu contraseña actual para cambiar la contraseña"

#: wp-password-policy-manager.php:281
msgid "New password must"
msgstr "La nueva contraseña debe"

#: wp-password-policy-manager.php:395
msgid "<strong>ERROR</strong>: Current password is incorrect."
msgstr "<strong>ERROR</strong>: La contraseña actual es incorrecta."

#: wp-password-policy-manager.php:435
msgid "The form is not valid. Please refresh the page and try again."
msgstr "El formulario no es válido. Por favor recarga la página y vuélvelo a intentar."

#: wp-password-policy-manager.php:439
msgid "Please provide your new password."
msgstr "Por favor introduce una nueva contraseña."

#: wp-password-policy-manager.php:443
msgid "Please confirm your new password."
msgstr "Por favor confirma tu nueva contraseña."

#: wp-password-policy-manager.php:451
msgid "Passwords must match."
msgstr "Las contraseñas tienen que coincidir."

#: wp-password-policy-manager.php:457
msgid "The new password cannot be the same as the current one."
msgstr "La contraseña nueva no puede ser igual a la actual."

#: wp-password-policy-manager.php:637
msgid "Password policy expiration time is not valid."
msgstr "El periodo de expiración no es válido."

#: wp-password-policy-manager.php:742
msgid "<p>Your password for <strong>%s</strong> has been reset.</p>"
msgstr "<p>Tu contraseña para <strong>%s</strong> se ha reseteado.</p>"

#: wp-password-policy-manager.php:743
msgid "<p>New Password: <strong>%s</strong></p>"
msgstr "<p>Nueva contraseña: <strong>%s</strong></p>"

#: wp-password-policy-manager.php:744
msgid "<p>Please log in and change your password:"
msgstr "<p>Por favor logueate y cambia tu contraseña:"

#: wp-password-policy-manager.php:759
msgid "Password has been reset"
msgstr "La contraseña se ha reseteado."

#: wp-password-policy-manager.php:783
msgid "You do not have sufficient permissions to access this page."
msgstr "No tienes suficientes permisos para acceder a esta página."

#: wp-password-policy-manager.php:791
msgid "Settings saved."
msgstr "Configuración guardada."

#: wp-password-policy-manager.php:795
msgid "All passwords have been reset."
msgstr "Todas las contraseñas se han reseteado."

#: wp-password-policy-manager.php:806
msgid "WordPress Password Policy Manager Settings"
msgstr "Configuración WordPress Password Policy Manager"

#: wp-password-policy-manager.php:824
msgid "Password Expiration Policy"
msgstr "Política de expiración de contraseñas"

#: wp-password-policy-manager.php:828
msgid "Examples: <code>5 days</code> <code>20 days 6 hours</code> <code>3 weeks</code>"
msgstr "Ejemplos: <code>5 days</code> <code>20 days 6 hours</code> <code>3 weeks</code>"

#: wp-password-policy-manager.php:829
msgid "Leave blank to disable Password Expiry policy."
msgstr "Dejar en blanco para deshabilitar la política de expiración de contraseña."

#: wp-password-policy-manager.php:833
msgid "Password Length Policy"
msgstr "Política de longitud de contraseña"

#: wp-password-policy-manager.php:843
msgid "characters"
msgstr "caracteres"

#: wp-password-policy-manager.php:844
msgid "Leave blank to disable Password Length policy."
msgstr "Dejar en blanco para deshabilitar la política de longitud de contraseña."

#: wp-password-policy-manager.php:848 wp-password-policy-manager.php:851
msgid "Mixed Case Policy"
msgstr "Política de mayúsculas y minúsculas"

#: wp-password-policy-manager.php:855
msgid "Password must contain a mix of uppercase and lowercase characters."
msgstr "La contraseña tiene que contener mayúsculas y minúsculas."

#: wp-password-policy-manager.php:861 wp-password-policy-manager.php:864
msgid "Numeric Digits Policy"
msgstr "Política de números"

#: wp-password-policy-manager.php:868
msgid "Password must contain numeric digits (<code>0-9</code>)."
msgstr "La contrasela tiene que contener números  (<code>0-9</code>)."

#: wp-password-policy-manager.php:874 wp-password-policy-manager.php:877
msgid "Special Characters Policy"
msgstr "Política de caracteres especiales"

#: wp-password-policy-manager.php:881
msgid "Password must contain special characters (eg: <code>.,!#$_+</code>)."
msgstr ""
"La contraseña tiene que contener caracteres especiales (por ejemplo: <code>.,!#$_+</code>)."

#: wp-password-policy-manager.php:887 wp-password-policy-manager.php:890
msgid "Current Password Policy"
msgstr "Política de contraseña actual"

#: wp-password-policy-manager.php:894
msgid "When changing password on the profile page, the user must supply the current password."
msgstr ""
"Cuando se cambia una contraseña en una página de perfil se tiene que introducir la contraseña "
"actual."

#: wp-password-policy-manager.php:900
msgid "Password History Policy"
msgstr "Política de historial de contraseñas"

#: wp-password-policy-manager.php:903
msgid "Remember"
msgstr "Recordar"

#: wp-password-policy-manager.php:912
msgid "old passwords"
msgstr "contraseñas antiguas"

#: wp-password-policy-manager.php:913
msgid "Leave blank to disable password history policy."
msgstr "Dejar en blanco para deshabilitar el histórico de contraseñas."

#: wp-password-policy-manager.php:918
msgid "Users and Roles Exempt From Policies"
msgstr "Usuarios y roles excluidos de las políticas"

#: wp-password-policy-manager.php:925
msgid "Users and Roles in this list are free of all Password Policies."
msgstr "Usuarios y roles en esta lista están libres de todas las políticas de contraseña."

#: wp-password-policy-manager.php:940
msgid "Reset All Users' Passwords"
msgstr "Resetar todas las contraseñas de los usuarios"

#: wp-password-policy-manager.php:1075 wp-password-policy-manager.php:1100
msgid "Configure Password Policies"
msgstr "Configurar las políticas de contraseñas"

#: wp-password-policy-manager.php:1081 wp-password-policy-manager.php:1084
#: wp-password-policy-manager.php:1085
msgid "Password Policies"
msgstr "Políticas de contraseñas"

#: wp-password-policy-manager.php:1101
msgid ""
"You have just installed WP Password Policy manager. All password policies are disabled by "
"default. Click the button below to configure the WordPress password policies."
msgstr ""
"Acabas de instalar WordPress Password Policy Manager. Todas las políticas de contraseña están "
"deshabilitadas por defecto. Haz click en el botón para configurar las políticas de contraseña de "
"WordPress."

#: wp-password-policy-manager.php:1102
msgid "Configure Policies"
msgstr "Configurar políticas"

#: wp-password-policy-manager.php:1118
msgid "Access Denied."
msgstr "Acceso denegado."

#: wp-password-policy-manager.php:1120
msgid "Token parameter expected."
msgstr "Se esperaba un parámetro de token."

